{
  "name": "Catalyst",
  "type": "dark",
  "colors": {
		"foreground": "#d4d4d8",
		"editor.background": "#09090b",
		"editor.foreground": "#f9fafb",
		"focusBorder": "#0ea5e9",
		"editor.lineHighlightBackground": "#18181b",
		"editor.lineHighlightBorder": "#00000000",
		"editorGutter.addedBackground": "#34d399",
		"editorGutter.deletedBackground": "#fb7185",
		"editorGutter.modifiedBackground": "#60a5fa",
		"editorLineNumber.foreground": "#52525b",
		"editorLineNumber.activeForeground": "#a1a1aa",
		"editorWhitespace.foreground": "#FFFFFF1A",
		"diffEditor.insertedLineBackground": "#2DD4BF26",
		"diffEditor.insertedTextBackground": "#34d39940",
		"diffEditor.removedTextBackground": "#fb718540",
		"diffEditor.removedLineBackground": "#fb718540",
		"terminal.background": "#18181b",
		"terminal.foreground": "#f3f4f6",
		"terminalCommandDecoration.successBackground": "#34d39940",
		"terminalCommandDecoration.errorBackground": "#fb718540",
		"terminal.ansiBlack": "#ffffff",
		"terminal.ansiBrightBlack": "#94a3b8",
		"terminal.ansiWhite": "#cbd5e1",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBlue": "#3b82f6",
		"terminal.ansiBrightBlue": "#60a5fa",
		"terminal.ansiCyan": "#22d3ee",
		"terminal.ansiBrightCyan": "#67e8f9",
		"terminal.ansiGreen": "#34d399",
		"terminal.ansiBrightGreen": "#6ee7b7",
		"terminal.ansiMagenta": "#c084fc",
		"terminal.ansiBrightMagenta": "#d8b4fe",
		"terminal.ansiRed": "#fb7185",
		"terminal.ansiBrightRed": "#fb7185",
		"terminal.ansiYellow": "#fbbf24",
		"terminal.ansiBrightYellow": "#fcd34d",
		"panel.background": "#18181b",
		"panel.border": "#FFFFFF1A",
		"editorHoverWidget.background": "#18181b",
		"editorHoverWidget.border": "#FFFFFF1A",
		"sideBar.background": "#18181b",
		"sideBar.border": "#FFFFFF1A",
		"activityBar.background": "#18181b",
		"activityBar.foreground": "#FFFFFF",
		"activityBar.inactiveForeground": "#a1a1aa",
		"activityBar.border": "#FFFFFF1A",
		"activityBar.activeBorder": "#71717a",
		"activityBarBadge.background": "#1C3544",
		"activityBarBadge.foreground": "#7DD3FC",
		"badge.background": "#1C3544",
		"badge.foreground": "#7DD3FC",
		"titleBar.activeBackground": "#18181b",
		"titleBar.inactiveBackground": "#18181b",
		"titleBar.border": "#FFFFFF1A",
		"list.hoverBackground": "#2D2D30",
		"list.inactiveSelectionBackground": "#2D2D30",
		"list.activeSelectionBackground": "#ffffff26",
		"list.focusAndSelectionOutline": "#00000000",
		"list.errorForeground": "#fda4af",
		"editorGroupHeader.tabsBorder": "#FFFFFF1A",
		"quickInput.background": "#18181b",
		"tab.inactiveBackground": "#18181b",
		"tab.inactiveBorder": "#FFFFFF1A",
		"tab.activeBorderTop": "#fff",
		"tab.activeBorder": "#09090b",
		"tab.border": "#FFFFFF1A",
		"button.background": "#52525b",
		"button.border": "#5B5B63",
		"input.background": "#242427",
		"input.border": "#FFFFFF1A",
		"statusBar.background": "#2D2D30",
		"statusBar.noFolderBackground": "#2D2D30",
		"statusBar.border": "#FFFFFF1A",
		"statusBarItem.remoteBackground": "#0EA5E933",
		"statusBarItem.remoteForeground": "#7DD3FC",
		"editorGroupHeader.tabsBackground": "#18181b",
		"editorGroupHeader.noTabsBackground": "#18181b",
		"gitDecoration.modifiedResourceForeground": "#7dd3fc",
		"gitDecoration.deletedResourceForeground": "#fb7185",
		"gitDecoration.untrackedResourceForeground": "#bef264"
	},
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "Constant - eg. Number, Boolean, Colors",
      "scope": ["constant"],
      "settings": {
        "foreground": "#fde68a"
      }
    },
    {
      "name": "Entity - eg. Function, Type, Tag, Attribute Name",
      "scope": ["entity"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "Entity Name - eg. Function, Type, Tag, Attribute Name",
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Entity Name Function - eg. Function, Type, Tag, Attribute Name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#99f6e4"
      }
    },
    {
      "name": "Keyword - eg. Control (while, for), Operator (or)",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "Keyword Assignment - eg. =",
      "scope": ["keyword.operator.assignment"],
      "settings": {
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Storage - eg. Type (class, function, var)",
      "scope": ["storage"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "String - eg. Quoted Value, Interpolated Values, Regex",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Support - eg. Key in JSON",
      "scope": ["support"],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "Variable - eg. Named Import, Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#fda4af"
      }
    },
    {
      "name": "Variable Object - eg. Named Import, Variable",
      "scope": ["variable.other.constant.object"],
      "settings": {
        "foreground": "#fda4af"
      }
    },
    {
      "name": "[HTML] - Entity",
      "scope": ["text.html entity"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "[HTML] - Attribute Names",
      "scope": ["text.html entity.other.attribute-name"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[HTML] - Attribute Values",
      "scope": [
        "text.html string.quoted",
        "text.html punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "[HTML] - Punctation",
      "scope": [
        "text.html punctuation",
        "text.html keyword.operator",
        "text.html meta.tag"
      ],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[HTML] - Doctype",
      "scope": [
        "text.html meta.tag.sgml",
        "text.html meta.tag.sgml punctuation"
      ],
      "settings": {
        "foreground": "#93c5fdbf"
      }
    },
    {
      "name": "[HTML] - Function",
      "scope": ["text.html entity.name.function"],
      "settings": {
        "foreground": "#99f6e4"
      }
    },
    {
      "name": "[CSS] - Entity - eg. .class, #id, body, :root",
      "scope": [
        "source.css entity",
        "source.css entity.other.attribute-name.class",
        "source.css entity.other.attribute-name.id",
        "source.css entity.other.attribute-name.pseudo-class",
        "source.css punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "[CSS] - Meta Attribute - eg. [data-attribute]",
      "scope": [
        "source.css meta.attribute-selector",
        "source.css meta.attribute-selector punctuation.definition.string",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "[CSS] - @ - eg. @media",
      "scope": [
        "source.css keyword.control.at-rule",
        "source.css punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[CSS] - Meta Value - eg. value",
      "scope": [
        "source.css meta.property-value",
        "source.css meta.property-value.css punctuation.definition.constant",
        "source.css meta.property-list meta.at-rule",
        "source.css variable.parameter"
      ],
      "settings": {
        "foreground": "#f9fafb"
      }
    },
    {
      "name": "[CSS] - Constant - eg. 2.55, ease-out",
      "scope": ["source.css constant", "source.css support.constant"],
      "settings": {
        "foreground": "#f9fafb"
      }
    },
    {
      "name": "[CSS] - Meta Name - eg. font-family, 'name'",
      "scope": [
        "source.css meta.property-name",
        "source.css support.type.property-name",
        "source.css string"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "[CSS] - Function - eg. var(), rgba()",
      "scope": ["source.css support.function"],
      "settings": {
        "foreground": "#dbeafe"
      }
    },
    {
      "name": "[CSS] - Keyword - eg. =, and",
      "scope": ["source.css keyword.control", "source.css keyword.operator"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[CSS] - Unit - eg. px, %",
      "scope": ["source.css keyword.other.unit"],
      "settings": {
        "foreground": "#ccfbf1"
      }
    },
    {
      "name": "[CSS] - Variable - eg. --var",
      "scope": [
        "source.css variable",
        "source.css entity.other.attribute-name.placeholder",
        "source.css entity.other.attribute-name.placeholder punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#99f6e4"
      }
    },
    {
      "name": "[CSS] - Punctuation - eg. <> , . () []",
      "scope": [
        "source.css punctuation.section",
        "source.css punctuation.separator",
        "source.css punctuation.terminator",
        "source.css punctuation.definition.constant",
        "source.css meta.attribute-selector punctuation.definition",
        "source.css punctuation.definition.parameters",
        "source.css punctuation.access",
        "source.css meta.property-list"
      ],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[HTML in JS] - Entity",
      "scope": [
        "source.js meta.block entity.name.tag",
        "source.ts meta.block entity.name.tag",
        "source.tsx meta.block entity.name.tag"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "[HTML in JS] - Attribute Names",
      "scope": [
        "source.js meta.block entity.other.attribute-name",
        "source.ts meta.block entity.other.attribute-name",
        "source.tsx meta.block entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[HTML in JS] - Attribute Values",
      "scope": [
        "source.js meta.block string.quoted",
        "source.ts meta.block string.quoted",
        "source.tsx meta.block string.quoted",
        "source.js meta.block punctuation.definition.string",
        "source.ts meta.block punctuation.definition.string",
        "source.tsx meta.block punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "[HTML in JS] - Constant",
      "scope": [
        "source.js meta.block meta.jsx.children",
        "source.ts meta.block meta.jsx.children",
        "source.tsx meta.block meta.jsx.children"
      ],
      "settings": {
        "foreground": "#f9fafb"
      }
    },
    {
      "name": "[HTML in JS] - Expression",
      "scope": [
        "source.js meta.block meta.embedded.expression variable.other",
        "source.ts meta.block meta.embedded.expression variable.other",
        "source.tsx meta.block meta.embedded.expression variable.other",
        "source.js meta.block meta.embedded.expression variable.other.readwrite",
        "source.ts meta.block meta.embedded.expression variable.other.readwrite",
        "source.tsx meta.block meta.embedded.expression variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#fda4af"
      }
    },
    {
      "name": "[HTML in JS] - Expression Ternary",
      "scope": [
        "source.js meta.block keyword.operator.ternary",
        "source.ts meta.block keyword.operator.ternary",
        "source.tsx meta.block keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "[JSON] - Support",
      "scope": ["source.json support"],
      "settings": {
        "foreground": "#e5e7eb"
      }
    },
    {
      "name": "[JSON] - Constant",
      "scope": ["source.json constant"],
      "settings": {
        "foreground": "#fda4af"
      }
    },
    {
      "name": "[JSON] - Constant (Escaped) - eg. '",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "[JS] - Variable",
      "scope": ["variable.other.property.js"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[JS] - Import",
      "scope": ["meta.import keyword.control.import"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[JS] - Import From",
      "scope": ["meta.import keyword.control.from", "meta.import punctuation"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "[JS] - Import String - eg. '../location/file",
      "scope": [
        "meta.import string",
        "meta.import string punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e5e7eb"
      }
    },
    {
      "name": "[JS] - Import Variable - eg. React, Moment",
      "scope": ["meta.import variable.other.readwrite"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    }
  ],
  "semanticHighlighting": true
}
